<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_63dp"
    android:minHeight="@dimen/dimen_63dp"
    android:id="@+id/search_member_item"
    android:background="@color/white"
    android:orientation="vertical"
    android:gravity="center_vertical">

    <LinearLayout
        android:id="@+id/name_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_14dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="aa"
            android:gravity="center"
            android:textSize="@dimen/normal"
            android:textColor="@color/color_43484b"/>
        <ImageView
            android:id="@+id/vip_tip"
            android:layout_marginLeft="@dimen/dimen_5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/meb_vip_ic"/>
    </LinearLayout>

    <TextView
        android:id="@+id/tel_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_14dp"
        android:layout_marginTop="@dimen/dimen_6dp"
        android:text="13500000000"
        android:textColor="@color/color_9b9b9b"
        android:textSize="@dimen/small"
        />
</LinearLayout>
